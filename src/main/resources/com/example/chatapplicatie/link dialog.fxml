<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.collections.FXCollections?>

<?import java.lang.String?>
<?xml version="1.0" encoding="UTF-8"?>
<Dialog xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="com.example.chatapplicatie.LinkDialog" title="Link to Scrum Entity">
    <dialogPane>
        <DialogPane>
            <content>
                <VBox spacing="10">
                    <Label text="Selecteer een Scrum Element:"/>
                    <ListView fx:id="entityList" prefHeight="200" prefWidth="300"/>
                </VBox>
            </content>
            <buttonTypes>
                <ButtonType text="Link" buttonData="OK_DONE"/>
                <ButtonType text="Cancel" buttonData="CANCEL_CLOSE"/>
            </buttonTypes>
        </DialogPane>
    </dialogPane>
</Dialog>
